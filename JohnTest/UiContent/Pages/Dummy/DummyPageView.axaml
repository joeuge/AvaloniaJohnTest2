<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:dummy="clr-namespace:AppNs.UiContent.Pages.Dummy"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AppNs.UiContent.Pages.Dummy.DummyPageView">
  <Design.DataContext>
    <dummy:DummyPage/>
  </Design.DataContext>

  <Grid>
    <StackPanel>
      <Button Content="Toggle (TabItem | Window)" Command="{Binding ToggleCommand}"/>
      <Button Content="Confirm Dialog" Command="{Binding ConfirmCommand}"/>
    </StackPanel>
  </Grid>

</UserControl>
